<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet on_load="onToolbarAppletLoad" on_click="onButtonClick" buttonstyle="windows" buttonsize="26,25">
  <toolbar id="fieldbar">
    <button image="../images/Delete.svg" id="delete" tooltip="删除选中的字段" text="删除字段" />
    <separator />
    <button image="../images/TextField.svg" id="text" tooltip="新建文本" text="新建文本" />
    <button image="../images/Password.svg" id="password" tooltip="新建密码" text="新建密码" />
    <button image="../images/TextArea.svg" id="textarea" tooltip="新建文本区域" text="新建文本区域" />
    <button image="../images/Dropdown.svg" id="dropdown" tooltip="新建下拉表" text="新建下拉表" />
    <button image="../images/ListBox.svg" id="listbox single select" tooltip="新建列表框" text="新建列表框" />
    <button image="../images/ListBoxMulti.svg" id="listbox multi select" tooltip="新建多选列表框" text="新建多选列表框" />
    <button image="../images/Checkbox.svg" id="checkbox" tooltip="新的复选框" text="新的复选框" />
    <button image="../images/RadioButton.svg" id="radio button list" tooltip="新建单选按钮列表" text="新建单选按钮列表" />
    <button image="../images/Button.svg" id="button" tooltip="新建按钮" text="新建按钮" />
    <button image="../images/BusinessCalendarYear.svg" id="date" tooltip="新建日期" text="新建日期" />
    <button image="../images/Color.svg" id="color" tooltip="新建颜色" text="新建颜色" />
    <button image="../images/Image.svg" id="html" tooltip="新建HTML" text="新建HTML" />
    <button image="../images/xClassification.svg" id="xclass" tooltip="新xClass" text="新xClass" />
    <separator />
    <drop_down_button id="unused_ds" tooltip="未使用的属性字段" text="未使用的属性字段" />
    <drop_down_button id="snap-to-grid" tooltip="对齐到网格功能" text="Snap:" singlecheck="true">Off510</drop_down_button>
  </toolbar>
</toolbarapplet>