<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet on_load="onToolbarAppletLoad" on_click="onButtonClick" buttonstyle="windows" buttonsize="26,25">
  <toolbar id="viewerbar">
    <button image="../images/New.svg" id="new_report" tooltip="新建报表" />
    <button image="../images/EditItem.svg" id="edit_report" tooltip="编辑" />
    <button image="../images/SaveAs.svg" id="save_as_report" tooltip="保存报表..." />
    <button image="../images/Delete.svg" id="delete_report" tooltip="删除报表" />
    <separator />
    <button image="../images/Print.svg" id="print_report" tooltip="打印" />
    <separator />
    <button image="../images/PDF.svg" id="export_pdf" tooltip="导出到PDF" />
    <button image="../images/ExportToExcel.svg" id="export_excel" tooltip="导出到Excel" />
    <button image="../images/ExportToWord.svg" id="export_word" tooltip="导出到Word" />
    <button image="../images/CSV.svg" id="export_csv" tooltip="导出到CSV" />
    <button image="../images/XML.svg" id="export_xml" tooltip="导出到XML" />
    <separator />
    <choice width="auto" id="results" tooltip="最大结果数" text="Results">1 结果10 结果100 结果1000 结果10000 结果所有 结果</choice>
  </toolbar>
</toolbarapplet>